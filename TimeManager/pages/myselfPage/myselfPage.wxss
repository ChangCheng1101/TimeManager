.background{
  background-size: cover;
  height:550rpx;
  margin-top: 0rpx;
  display: flex;
  justify-content: center;
  overflow: hidden;
  position: relative;
  flex-direction: column;
  align-items: center;
}
.background image{
  width:100%;
}
.bg-gif{
  z-index: 99;
  mix-blend-mode: screen;
  position: absolute;
  bottom:0;
  height:35%;
}
